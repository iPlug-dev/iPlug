{
        "name": "iPlug BETA",
        "short_name": "iPlug",
        "author": "CookieMichal & L0laapk3",
        "version": "0.6.1",
        "manifest_version": 2,
        "description": "A free addon for http://plug.dj/ that allows autowoot, autojoin and many other things!",
        "icons": {
                "256": "images/Chrome/icon256.png"
        },
        "permissions": [
                "*://*.plug.dj/*",
                "storage",
                "notifications",
                "contextMenus"
        ],
        "optional_permissions": [
                "http://*/*",
                "https://*/*"
        ],
        "externally_connectable": {
                "matches": [
                        "*://plug.dj/*"
                ], "exclude_matches": [
                        "*://plug.dj/communities/",
                        "*://plug.dj/about",
                        "*://plug.dj/terms",
                        "*://plug.dj/privacy" ]
        },
